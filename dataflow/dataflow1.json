{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MoviesInput",
						"type": "DatasetReference"
					},
					"name": "moviesInput"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "MoviesOutput",
						"type": "DatasetReference"
					},
					"name": "moviesOutput"
				}
			],
			"transformations": [
				{
					"name": "YearExtracted",
					"description": "Create Year column"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movieId as string,",
				"          title as string,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> moviesInput",
				"moviesInput derive(Year = toInteger(trim(right(title,6),'()')),",
				"          title = toString(left(title,length(title)-6))) ~> YearExtracted",
				"YearExtracted sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> moviesOutput"
			]
		}
	}
}